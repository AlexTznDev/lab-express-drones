<h2>Available drones</h2>
<div>
    
        {{#each allDrones}}
            
             
             <ul>   
                <li><h4>{{this.name}}</h4></li>
                <p>Name: {{this.propellers}}</p>
                <p>Speed: {{this.maxSpeed}}</p>
                <a href="/drones/{{this._id}}/edit"><button>Update</button></a>
            </ul>

        {{/each}}
    
</div>